@model OrganWeb.Areas.Sistema.Models.ViewModels.ViewFornecedor
@{
    ViewBag.Title = "Fornecedores";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
Vi
<div class="main-body">
    <div class="row">
        <div class="col-md-8">
            <div class="card">
                <div class="card-header">
                    <h6 class="card-title box-title">Fornecedores</h6>
                    <div class="card-header-right">
                        <ul class="list-unstyled card-option">
                            <li><i class="ti-minus minimize-card"></i></li>
                            <li><i class="icon-refresh"></i></li>
                        </ul>
                    </div>
                </div>
                <div class="card-body">
                    <div class="card-content">
                        <div class="card bg-light mb-3" style="max-width: 18rem;">
                           
                                <table class="table">
                                    <thead class="thead-title">
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">Nome</th>
                                            <th scope="col">CNPJ</th>
                                            <th scope="col">RazaoSocial</th>
                                            <th scope="col">Site</th>
                                            <th scope="col">Email</th>
                                            <th scope="col">Status</th>
                                            <th scope="col">CEP</th>
                                            <th scope="col">Numero</th>
                                            <th scope="col">Localizacao</th>
                                          
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.Fornecedors)
                                        {
                                        <tr>
                                            <th scope="row"></th>
                                            <td>@Html.DisplayFor(modelItem => item.Nome)</td>
                                            <td>@Html.DisplayFor(modelItem => item.CNPJ)</td>
                                            <td>@Html.DisplayFor(modelItem => item.RazaoSocial)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Site)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Email)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Status)</td>
                                            <td>@Html.DisplayFor(modelItem => item.CEP)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Numero)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Localizacao)</td>
                                          

                                            @*nao sei como q coloca o telefone milena, vou lembrar de te perguntar*@

                                        </tr>
                                        }
                                    </tbody>
                                    @* <div class="modal-footer">
                <button type="button" class="btn btn-primary edit" data-toggle="newmodal" data-url="@Url.Action("Editar", "Manutencao", new { id = item.IdManuntencao, id2 = item.IdMaquina })">Editar</button>
                @*<button type="button" class="btn btn-primary edit" data-toggle="edit-modal" data-target="#edit-item">Editar</button>*@
                                    @* @{Html.RenderAction("Editar", "Manutencao", new { id = item.IdManuntencao, id2 = item.IdMaquina });}
                    <button type="button" class="btn btn-danger delete" data-url="@Url.Action("Excluir", "Manutencao", new { id = item.IdManuntencao, id2 = item.IdMaquina })">Excluir</button>
                </div>*@
                                    }
                            </div>
                    </div>
                </div>
                <div class="card-content">
                    <div class="card border-success mb-3" style="max-width: 18rem;">
                        <div class="card-body text-success">
                            @* <h5 class="card-title">Nova manutenção</h5>
        <p class="card-text">
            <div class="teste"></div>
            <button type="button" class="btn btn-info waves-effect" data-toggle="ajax-modal" data-target="#add-item">Nova manutenção</button>
            @{Html.RenderAction("_NovaManutencao", "Manutencao");}*@
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
